<div>
  <h3><%='These are all the groups created by the user' %></h3>
  <% @groups.each_with_index do |group, index| %>
    <ul class="list-unstyled">
   
      <li><%= link_to group.name, group, class: 'mx-3' %><%= raw group.icon %></li>
     
    </ul>
  <% end %>
  <h5><%= link_to 'All Groups', groups_path %></h5>
</div>